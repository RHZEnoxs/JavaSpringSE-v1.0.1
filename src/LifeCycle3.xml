<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
    xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context = "http://www.springframework.org/schema/context"
    xsi:schemaLocation = "http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd" >

    <context:annotation-config/>
    <!-- bean definitions go here -->

    <!-- Hello World Example -->
    <bean id = "objectTypeE" class = "com.enoxs.datadef.ObjectTypeE">
        <property name="message" value = "OBJ TYP E MSG#0"/>
    </bean>

    <bean id = "textSEv3_3" class = "com.enoxs.injection.TextSEv3_3"></bean>
    <bean id="spellChecker" class="com.enoxs.datadef.SpellChecker"></bean>
</beans>

